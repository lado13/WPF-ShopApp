<Window x:Class="WPFApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        xmlns:local="clr-namespace:WPFApp.View"
        xmlns:NavBar="clr-namespace:WPFApp.View"
        Title="Shopify" Height="500" Width="1200" Background="#0e0e0f"
        WindowStartupLocation="CenterScreen">
    <Grid>

        <Frame x:Name="MainFrame"/>
        <NavBar:NavBar Panel.ZIndex="4" Height="100"  VerticalAlignment="Top" ></NavBar:NavBar>
        <local:ContentUI Panel.ZIndex="1" Margin="0,120"></local:ContentUI>


    </Grid>
</Window>
